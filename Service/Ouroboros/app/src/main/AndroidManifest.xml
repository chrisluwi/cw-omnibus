<?xml version="1.0" encoding="utf-8"?>
<manifest package="com.commonsware.android.service.ouroboros"
  xmlns:android="http://schemas.android.com/apk/res/android">

  <application
    android:allowBackup="false"
    android:icon="@mipmap/ic_launcher"
    android:label="@string/app_name"
    android:supportsRtl="true"
    android:theme="@style/AppTheme">
    <activity
      android:name=".MainActivity"
      android:theme="@style/AppTheme">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>

    <service android:name=".FirstSillyService" />
    <service android:name=".SecondSillyService" />

    <receiver android:name=".HackReceiver">
      <intent-filter>
        <action android:name="com.commonsware.WTF" />
      </intent-filter>
    </receiver>
  </application>

</manifest>